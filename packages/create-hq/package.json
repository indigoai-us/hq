  "files": [
    "dist",
    "template"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "node -e \"const fs=require('fs-extra');fs.removeSync('dist');fs.removeSync('template')\"",
