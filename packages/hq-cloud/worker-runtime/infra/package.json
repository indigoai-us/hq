{
  "name": "@hq-cloud/worker-runtime-infra",
  "version": "0.1.0",
  "description": "AWS CDK infrastructure for HQ Worker Runtime",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
