  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.173.2",
    "constructs": "^10.4.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "hq",
    "worker",
    "aws",
    "cdk",
    "ecs",
    "fargate"
