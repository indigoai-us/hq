{
  "name": "@hq-cloud/worker-runtime-types",
  "version": "0.1.0",
  "description": "TypeScript types for HQ Worker Runtime infrastructure",
  "type": "module",
  "main": "dist/infra/index.js",
  "types": "dist/infra/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/infra/index.js",
      "types": "./dist/infra/index.d.ts"
    },
    "./infra": {
      "import": "./dist/infra/index.js",
      "types": "./dist/infra/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint infra --ext .ts",
    "lint:fix": "eslint infra --ext .ts --fix",
    "format": "prettier --write infra",
    "format:check": "prettier --check infra"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "hq",
    "worker",
    "types",
    "typescript"
  ],
  "author": "HQ Cloud Team",
  "license": "UNLICENSED"
}
