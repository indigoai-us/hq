{
  "name": "hq-cloud",
  "private": true,
  "version": "0.1.0",
  "description": "Cloud infrastructure for running HQ workers remotely",
  "scripts": {
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint",
