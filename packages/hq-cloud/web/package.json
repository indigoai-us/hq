{
  "name": "@hq-cloud/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-http-header-size=32768 next dev",
    "build": "next build",
    "start": "cross-env NODE_OPTIONS=--max-http-header-size=32768 next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test --ignore-snapshots",
    "test:e2e:ci": "cross-env E2E_MOCK_ONLY=true CI=true playwright test",
    "test:e2e:ci:full": "cross-env CI=true playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:smoke": "playwright test e2e/smoke.spec.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.0.0",
    "@clerk/themes": "^2.0.0",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.500.0",
    "@clerk/testing": "^1.5.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.1.0",
    "happy-dom": "^15.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
